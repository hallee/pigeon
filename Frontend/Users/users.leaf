<!doctype html>
<html lang="en">
	<head>
		<title>Pigeon CMS</title>
	</head>
	<body>
		<nav>
			#embed("navigation")
			#embed("administration")
		</nav>
		<img src="/images/pigeon.svg" />
		<h1>Users</h1>
