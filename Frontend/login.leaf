<!doctype html>
<html lang="en">
    <head>
        <title>Pigeon CMS | Log In</title>
    </head>
    <body>
        <img src="/images/pigeon.svg" />
        <form
        	action="/login"
        	method="POST">
        	<input
        		type="email"
        		name="email"
        		id="email"
        	/>
        	<input
        		type="password"
        		name="password"
        		id="password"
        	/>
        	<input
        		type="submit"
        		value="login"
        	/>
        </form>
    </body>
</html>