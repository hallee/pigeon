<!doctype html>
<html lang="en">
	<head>
		<title>Pigeon CMS</title>
	</head>
	<body>
		<nav>
			#embed("administration")
		</nav>
		<img src="/images/pigeon.svg" />
		<h1>Content Types</h1>
		#for(type in contentTypes) {
			<div>
				Content Type #(type.name)
			</div>
		}
		#if(count(contentTypes) == 0) {
			#embed("create-content")
		}
	</body>
</html>
